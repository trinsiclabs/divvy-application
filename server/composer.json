{
    "name": "trinsiclabs/app",
    "type": "silverstripe-recipe",
    "description": "Trinsic Labs: App Starter Kit",
    "require": {
        "php": ">=7.1.0",
        "silverstripe/recipe-core": "^4.4",
        "silverstripe/environmentcheck": "^2.1",
        "dnadesign/silverstripe-populate": "^2.0",
        "silverstripe/siteconfig": "^4.4",
        "silverstripe/login-forms": "^4.0@beta",
        "silverstripe/graphql": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "lekoala/silverstripe-debugbar": "^2.0",
        "silverstripe/graphql-devtools": "^1.0@dev"
    },
    "extra": {
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/mysite.yml"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "install-frameworkmissing.html",
            "install.php",
            "web.config"
        ]
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "TrinsicLabs\\App\\": "app/src/"
        },
        "files": [
            "app/src/includes/constants.php"
        ]
    }
}
